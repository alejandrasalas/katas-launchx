    "\n",
    "new_planet = ''\n",
    "planets = []\n",
    "# Escribe el ciclo while solicitado\n",
    "\n",
    "while new_planet.lower() != 'terminar':\n",
    "    if new_planet:\n",
    "        planets.append(new_planet)\n",
    "    new_planet = input('ingresar nuevo planeta ')\n",
    "\n",
    "\n",
    "\n",
    "for planet in planets:\n",
    "    print(planet)\n"
